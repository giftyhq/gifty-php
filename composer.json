{
  "name": "gifty/gifty-php",
  "description": "PHP library for interacting with the Gifty API.",
  "type": "library",
  "keywords": [
    "gifty",
    "api"
  ],
  "homepage": "https://gifty.nl",
  "license": "MIT",
  "authors": [
    {
      "name": "Gifty B.V.",
      "email": "info@gifty.nl"
    }
  ],
  "support": {
    "email": "info@gifty.nl"
  },
  "require": {
    "php": ">=7.2.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "squizlabs/php_codesniffer": "^3.5",
    "phpstan/phpstan": "^0.12.42",
    "dg/bypass-finals": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "Gifty\\Client\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gifty\\Tests\\": "tests/"
    }
  }
}
